{
  "name": "@computekit/wasm",
  "version": "0.1.0",
  "description": "WASM compute functions for ComputeKit",
  "type": "module",
  "main": "./build/release.js",
  "types": "./build/release.d.ts",
  "exports": {
    ".": {
      "types": "./build/release.d.ts",
      "import": "./build/release.js"
    },
    "./wasm": "./build/release.wasm"
  },
  "files": [
    "build",
    "assembly"
