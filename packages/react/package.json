{
  "name": "@computekit/react",
  "version": "0.2.0",
  "description": "React bindings for ComputeKit - WASM + Worker toolkit",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@computekit/core": "*"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "@types/react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "react",
    "wasm",
    "webassembly",
    "workers",
    "hooks",
    "compute",
    "performance"
  ],
  "author": "Ghassen Lassoued <https://github.com/tapava>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tapava/compute-kit",
    "directory": "packages/react"
  },
  "sideEffects": false
}
